{
  "name": "GazpromExchCNYTHB",
  "version": "2.4.0",
  "description": "Telegram bot to monitor the cross rate between the RUB and the THB via the CNY. Only for Gazprombank clients.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config src/index.ts dotenv_config_path=./.env",
    "start": "node -r dotenv/config src/index.ts dotenv_config_path=./.env",
    "build": "tsc --project tsconfig.json --listFiles --pretty",
    "start-on": "npx ts-node build/index.js",
    "start-forever": "forever start --minUptime 1000 --spinSleepTime 1000 -w -c ts-node build/index.js"
  },
  "keywords": [],
  "author": "Daria Zaitseva",
  "license": "MIT",
  "dependencies": {
    "@grammyjs/conversations": "^1.1.1",
    "@grammyjs/parse-mode": "^1.6.0",
    "@typegoose/typegoose": "^7.6.1",
    "@types/body-parser": "^1.19.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.16",
    "@types/node": "^15.12.4",
    "axios": "^1.3.0",
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^10.0.0",
    "express": "^4.18.2",
    "forever": "^4.0.3",
    "grammy": "^1.13.1",
    "mongoose": "^5.13.2",
    "nodemon": "^2.0.20",
    "typegram": "^3.7.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "babel-cli": "^6.26.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-typescript": "^0.14.0",
    "ts-node": "^10.9.1"
  }
}
